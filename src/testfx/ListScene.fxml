<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testfx.ListSceneController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ListView fx:id="list1" prefHeight="550.0" prefWidth="260.0">
                     <HBox.margin>
                        <Insets top="50.0" />
                     </HBox.margin></ListView>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Button fx:id="toRightButton" mnemonicParsing="false" onAction="#toRightAction" prefHeight="41.0" prefWidth="70.0" text="&gt;">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                           <font>
                              <Font name="AppleGothic Regular" size="20.0" />
                           </font>
                        </Button>
                        <Button fx:id="toLeftButton" mnemonicParsing="false" onAction="#toLeftAction" prefHeight="40.0" prefWidth="70.0" text="&lt;">
                           <font>
                              <Font name="AppleGothic Regular" size="20.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
                  <ListView fx:id="list2" prefHeight="550.0" prefWidth="260.0">
                     <HBox.margin>
                        <Insets top="50.0" />
                     </HBox.margin></ListView>
               </children>
            </HBox>
         </center>
         <left>
            <VBox prefHeight="400.0" prefWidth="160.0" style="-fx-background-color: #707070;" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="homeMenuButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#homeMenuButtonAction" prefHeight="50.0" prefWidth="200.0" text="&lt;">
                     <font>
                        <Font name="AppleGothic Regular" size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="addQuestionsMenuButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#addQuestionsMenuButtonAction" prefHeight="50.0" prefWidth="200.0" text="Lägg till frågor">
                     <font>
                        <Font name="Hiragino Sans W1" size="15.0" />
                     </font>
                  </Button>
                  <Button fx:id="showTableMenuButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#showTableMenuButtonAction" prefHeight="50.0" prefWidth="200.0" text="Table">
                     <font>
                        <Font name="Hiragino Sans W1" size="15.0" />
                     </font>
                  </Button>
                  <Button alignment="BASELINE_LEFT" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #1d92b0;" text="Skapa test">
                     <font>
                        <Font name="Hiragino Sans W1" size="15.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
